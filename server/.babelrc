{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "node": "14.15.5"
        }
      }
    ],
    [
      "@babel/typescript",
      {
        "targets": {
          "node": "14.15.5"
        }
      }
    ]
  ],
  "plugins": [
    "@babel/plugin-transform-runtime",
    "@babel/plugin-transform-typescript",
    ["@babel/plugin-proposal-decorators", { "legacy": true }],
    "@babel/plugin-proposal-class-properties",
    "@babel/proposal-object-rest-spread"
  ]
}
